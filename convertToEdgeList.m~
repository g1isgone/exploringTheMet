function edgeList = convertToEdgeList(adjMatrix) 
numEdges = sum(adjMatrix(:)); 
edgeList = zeros(numEdges,2); 
edgeIndex = 1; 
for i = 1:size(original,1) 
    row = find(original(i, :) > 0); 
    for 1:numel(row) 
        edgeList(edgeIndex, 1) = i; 
        edgeList(edgeIndex, 2) = row(
    end
end
end